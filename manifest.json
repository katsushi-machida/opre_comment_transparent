{
  "name": "opre_comment_transparent",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "display openrec comment to full screen.",
  "icons": {
      "48" : "48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.openrec.tv/live/*"
      ],
      "js": [
        "scripts/resource/jquery-3.2.1.min.js",
        "scripts/contentscript.js"
      ],
      "run_at": "document_end"
    }
  ]
}
